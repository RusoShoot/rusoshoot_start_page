{
  "username": "RusoShoot",
  "terminal": {
    "fixedHeight": true,
    "backgroundColor": "#121317",
    "windowColor": "#1e212b",
    "glowColor": "#6b5cb157"
  },
  "prompt": {
    "ctrlC": true,
    "placeholder": "command...",
    "userColor": "green",
    "atColor": "gray",
    "hostColor": "magenta",
    "promptColor": "magenta",
    "promptSymbol": "‚ùØ",
    "caretColor": "green",
    "selectionBg": "#e8b195",
    "selectionFg": "#16161e"
  },
  "colors": {
    "white": "#e2e2e2",
    "gray": "#97989d",
    "black": "#16161e",
